/* pages/dataImport/dataImport.wxss */
.container{
  display: block;
  padding: 20px 20px;
}

.info{
  position: relative;
  display: flex;
  height: 40px;
  width: 80%;
  padding: 0 20px;
  border: 1px solid #EEE;
  margin-bottom: 50px;
  align-items: center;
  justify-content: flex-start;
  border-radius: 5px;
  box-shadow: 0 2px 10px #CCC;
}

.info-icon{
  width: 15px;
  height: 15px;
  border-radius: 10px;
  margin-right: 10px;
}